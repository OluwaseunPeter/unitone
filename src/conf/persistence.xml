<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="NeoForcePU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.tp.neo.model.Agent</class>
    <class>com.tp.neo.model.Customer</class>
    <class>com.tp.neo.model.Agent_1</class>
    <class>com.tp.neo.model.CustomerAgent</class>
    <class>com.tp.neo.model.CustomerAgent_1</class>
    <class>com.tp.neo.model.Customer_1</class>
    <class>com.tp.neo.model.Lodgement</class>
    <class>com.tp.neo.model.Sale</class>
    <class>com.tp.neo.model.Sale_1</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/xforce?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
    </properties>
  </persistence-unit>
</persistence>
